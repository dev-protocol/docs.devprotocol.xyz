{
	"name": "docs",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"docusaurus": "docusaurus",
		"start": "docusaurus start",
		"build": "docusaurus build",
		"swizzle": "docusaurus swizzle",
		"deploy": "docusaurus deploy",
		"clear": "docusaurus clear",
		"serve": "docusaurus serve",
		"write-translations": "docusaurus write-translations",
		"write-heading-ids": "docusaurus write-heading-ids",
		"ci": "yarn lint && yarn format:diff",
		"lint": "eslint --cache \"**/*.js\" && stylelint --fix \"**/*.css\"",
		"format": "prettier --cache --config .prettierrc.json --write \"**/*.{js,jsx,ts,tsx,md,mdx,json,yml}\"",
		"format:diff": "prettier --config .prettierrc.json --list-different \"**/*.{js,jsx,ts,tsx,md,mdx,json,yml}\"",
		"prepare": "husky install"
	},
	"dependencies": {
		"@docusaurus/core": "2.0.0-beta.21",
		"@docusaurus/preset-classic": "2.0.0-beta.21",
		"@mdx-js/react": "^2.1.2",
		"clsx": "^1.1.1",
		"prism-react-renderer": "^1.3.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.18.2",
		"eslint": "^8.18.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-header": "^3.1.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-react": "^7.30.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.1",
		"prettier": "^2.7.1",
		"stylelint": "14.9.1",
		"stylelint-config-standard": "^26.0.0"
	},
	"browserslist": {
		"production": [
			">0.5%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
