{
	"name": "docs",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"docusaurus": "docusaurus",
		"start": "docusaurus start",
		"build": "docusaurus build",
		"swizzle": "docusaurus swizzle",
		"deploy": "docusaurus deploy",
		"clear": "docusaurus clear",
		"serve": "docusaurus serve",
		"write-translations": "docusaurus write-translations",
		"write-heading-ids": "docusaurus write-heading-ids",
		"ci": "yarn lint && yarn format:diff",
		"lint": "eslint --cache \"**/*.js\" && stylelint --fix \"**/*.css\"",
		"format": "prettier --config .prettierrc.json --write \"**/*.{js,jsx,ts,tsx,md,mdx,json,yml}\" --cache",
		"format:diff": "prettier --config .prettierrc.json --list-different \"**/*.{js,jsx,ts,tsx,md,mdx,json,yml}\"",
		"prepare": "husky install"
	},
	"dependencies": {
		"@docusaurus/core": "^2.0.1",
		"@docusaurus/preset-classic": "^2.0.1",
		"@mdx-js/react": "^1.6.21",
		"clsx": "^1.1.1",
		"prism-react-renderer": "^1.3.3",
		"react": "^17.0.1",
		"react-dom": "^17.0.1"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.16.3",
		"dotenv": "^16.0.1",
		"eslint": "^8.2.0",
		"eslint-config-airbnb": "^19.0.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-header": "^3.1.1",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-react": "^7.27.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"husky": "^8.0.1",
		"prettier": "^2.5.1",
		"stylelint": "14.8.3",
		"stylelint-config-standard": "^25.0.0"
	},
	"browserslist": {
		"production": [
			">0.5%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
